'''
# Ass 1 ( Greatest of three)
def larg(num1, num2, num3):
    if (num1>num2):
        if (num1>num3):
            return num1
        else:
            return num3
         
    else:
         if (num2>num3):
             return num2
         else:
             return num3
n = larg(1, 12, 6)
print("Largest is " + str(n))
'''

'''
# Ass 2 (Degree to farenhite temperature)
def faren(cel):
    far = (cel * (9/5)) + 32 
    return far

d = float(input("Enter temperature in dergee: "))
f = faren(d)
print("Dergee of " + str(d) + " is "+ str(f))
'''
